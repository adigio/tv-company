        <mxCell id="ZU4GmCSSWKOK-8EI7Xn7-11" value="webapps/WEB-INF/views/thymeleaf" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
